{
  "name": "web-app",
  "version": "1.0.0",
  "description": "SQL Client for MongoDB [Web app]",
  "scripts": {
    "vagrant-start": "cd scripts/vagrant/ && vagrant up",
    "vagrant-stop": "cd scripts/vagrant/ && vagrant halt -f",
    "vagrant-reload": "cd scripts/vagrant/ && vagrant reload",
    "vagrant-destroy": "cd scripts/vagrant/ && vagrant destroy -f",
    "vagrant-recreate": "cd scripts/vagrant/ && vagrant destroy -f && vagrant up",
    "vagrant-connect": "cd scripts/vagrant/ && vagrant ssh",
    "test": "karma start tests/karma/karma.conf.js",
    "selenium-server": "webdriver-manager update && webdriver-manager start",
    "selenium": "protractor tests/selenium/selenium.conf.js"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.22.0",
    "bower": "1.8.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-autoprefixer": "4.0.0",
    "gulp-babel": "6.1.2",
    "gulp-clean": "0.3.2",
    "gulp-concat": "2.6.1",
    "gulp-copy": "1.0.0",
    "gulp-cssnano": "2.1.2",
    "gulp-if": "2.0.2",
    "gulp-ng-annotate": "2.0.0",
    "gulp-ngmin": "0.3.0",
    "gulp-put": "0.0.3",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.1.0",
    "gulp-sourcemaps": "2.6.0",
    "jasmine-core": "2.6.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-jasmine": "1.1.0",
    "karma-spec-reporter": "0.0.31",
    "protractor": "5.1.2",
    "yargs": "8.0.2"
  }
}
